server {
    listen      80;
    server_name victim victim.no-ip.org;
    charset     utf-8;
    client_max_body_size 75M;

    location /api/wizard/ {
        include uwsgi_params;
        uwsgi_pass unix:///opt/gdas/var/wizard.sock;
    }

    location /api/core/ {
        include uwsgi_params;
        uwsgi_pass unix:///opt/gdas/var/simplePassiveCore.sock;
    }
}
